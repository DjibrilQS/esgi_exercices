<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/viewParking"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:fontFamily="@font/jakarta"
        android:orientation="vertical"
        android:textAlignment="center"
        android:text="Parking Receipt"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold"
        />
    <View
        android:id="@+id/view2"
        android:layout_width="316dp"
        android:layout_height="422dp"
        android:background="@android:color/white"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="154dp" />

    <TextView
        android:layout_width="203dp"
        android:layout_height="41dp"
        android:text="Surdinam District Space"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/viewParkingImage"
        tools:layout_editor_absoluteY="179dp" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/viewParkingImage"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="24dp"
        android:src="@drawable/illu_parking"
        app:layout_constraintStart_toStartOf="@+id/view2"
        tools:layout_editor_absoluteY="170dp" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:layout_width="316dp"
        android:layout_height="43dp"
        android:src="@drawable/bg_dashed_line"
        app:layout_constraintTop_toBottomOf="@+id/viewParkingImage"
        tools:layout_editor_absoluteX="47dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
